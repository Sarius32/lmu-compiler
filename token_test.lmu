class Car {
    int wheels = 0;

    method addWheel() {
        this.wheels = this.wheels + 1;
        return this.wheels;
    }

    method removeWheel() {
        if ( this.wheels > 0 ) then {
            this.wheels = this.wheels - 1;
        }
        return this.wheels;
    }

    method printWheels() {
        print( this.wheels );
    }

    method printWheelsRep( num ) {
        int i = 0;
        while (i < num) do {
            this.printWheels();
            i = i+1;
        }
    }
}

